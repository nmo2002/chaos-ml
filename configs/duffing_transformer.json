{
  "seed": 1,
  "output_dir": "runs/duffing_transformer",
  "plot": true,
  "system": {
    "name": "duffing",
    "t_end": 100,
    "t_points": 2000,
    "lyapunov_exponent": 1.0,
    "initial_conditions": [0.0, 0.0],
    "params": {
      "delta": 0.02,
      "alpha": 5.0,
      "beta": 1.0,
      "gamma": 7.0,
      "omega": 0.5
    },
    "observable": "x"
  },
  "data": {
    "window": 20,
    "horizon": 1,
    "stride": 1,
    "train_ratio": 0.2,
    "val_ratio": 0.1,
    "scaler": "minmax",
    "feature_range": [0.0, 1.0]
  },
  "model": {
    "name": "transformer",
    "num_layers": 3,
    "d_model": 64,
    "num_heads": 4,
    "dff": 128,
    "dropout": 0.1
  },
  "training": {
    "epochs": 300,
    "batch_size": 64,
    "patience": 30,
    "learning_rate": 0.001
  }
}
